{
	"name": "Omniki",
	"version": "0.0.0",
	"manifest_version": 2,
	"browser_action": {
		"default_icon": "assets/icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [ "dist/content.js" ]
		}
	],
	"background": {
		"scripts": [ "dist/background.js" ],
		"persistent": false
	},
	"commands": {
		"aoeu": {
			"suggested_key": {
				"default": "Alt+T"
			},
			"description": "Toggle feature foo"
		}
	}
}
